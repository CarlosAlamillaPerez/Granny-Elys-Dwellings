@model Room

<h1>@Model.Name</h1>

<img src="@Model.ImageUrl" alt="@Model.Name">
<p>Precio: $@Model.Price</p>
<p>Descripción: @Model.Description</p>
<p>Dirección: @Model.Address</p>

<h3>Servicios:</h3>
<ul>
    @foreach (var service in Model.Services)
    {
        <li>@service</li>
    }
</ul>

<a href="https://wa.me/NUMERODEWHATSAPP?text=Me interesa la habitación @Model.Name" target="_blank">Contactar por WhatsApp</a>